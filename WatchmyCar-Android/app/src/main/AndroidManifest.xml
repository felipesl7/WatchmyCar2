<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.felipesantos.watchmycar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"
            android:label= "@string/login"/>
        <activity android:name=".ActivityAcompanhamento"
            android:label="@string/acompanhamento"/>
        <activity android:name=".ActivityIniciarViagem"
            android:label="@string/viagem"/>
        <activity android:name=".ActivityCadastrarVeiculo"
            android:label="@string/cadastro"/>
        <activity android:name=".ActivityVisualizarViagens"
            android:label="@string/listViagens"></activity>
    </application>

</manifest>